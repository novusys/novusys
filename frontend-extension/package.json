{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "watch": {
    "build": {
      "patterns": [
        ".",
        "styles/**",
        "pages/**",
        "public/**",
        "layouts/**",
        "components/**"
      ],
      "ignore": [
        "out",
        "node_modules",
        ".next"
      ],
      "extensions": [
        "js",
        "json",
        "lock",
        "tsx",
        "ts",
        "scss",
        "css"
      ]
    }
  },
  "scripts": {
    "dev": "npm-watch",
    "build": "next build && next export && npm run lint:build",
    "lint:build": "move out/_next out/assets && sed -i \"s/\\/_next/\\/assets/g\" out/**.html"
  },
  "dependencies": {
    "@nextui-org/react": "^1.0.0-beta.12",
    "@types/chrome": "^0.0.224",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "next": "13.2.4",
    "npm-watch": "^0.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "sass": "^1.59.3",
    "typescript": "5.0.2"
  }
}
